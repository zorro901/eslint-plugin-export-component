{
    "compilerOptions": {
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "jsx": "react-jsx",
        "lib": ["ES2023"],
        "module": "ESNext",
        "outDir": "dist",
        "removeComments": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "strict": true,
        "target": "ES2022",
        "moduleResolution": "bundler"
    },
    "include": ["src"]
}
